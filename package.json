{
  "imports": {
    "~/*": "./*.ts"
  },
  "scripts": {
    "build": "vite build && pnpm prisma:generate",
    "ci": "pnpm prisma:generate && pnpm run prettier:check && pnpm run lint && tsc --noEmit && sort-package-json package.json --check && pnpm test",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/server/index.ts",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "prettier:check": "prettier ./src --check",
    "prettier:format": "prettier ./src --write",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "server:prod": "cross-env NODE_ENV=production ts-node ./src/server",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
