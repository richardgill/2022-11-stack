    "express": "^4.18.1",
    "focus-visible": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "postcss-focus-visible": "^7.1.0",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sirv": "^2.0.2",
    "sort-package-json": "^2.0.0",
    "superjson": "^1.11.0",
